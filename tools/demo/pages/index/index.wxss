.container {
  width: 100vw;
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 0 30rpx 100rpx;
  box-sizing: border-box;
}
.title {
  margin-top: 60rpx;
  font-size: 30rpx;
  margin-bottom: 12rpx;
  font-weight: bold;
}
.image-box {
  border: 1px solid #CDCDCD;
  border-radius: 10rpx;
  width: 400rpx;
}
.image {
  width: 400rpx;
  height: 400rpx;
}